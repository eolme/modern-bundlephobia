{
  "root": true,
  "extends": [
    "@mntm"
  ],
  "parserOptions": {
    "project": "./tsconfig.json",
    "tsconfigRootDir": "."
  },
  "rules": {
    "capitalized-comments": 0,
    "sonarjs/no-duplicate-string": 0,
    "react-hooks/exhaustive-deps": 0,
    "unicorn/prefer-string-starts-ends-with": 0,
    "@typescript-eslint/prefer-string-starts-ends-with": 0
  }
}
